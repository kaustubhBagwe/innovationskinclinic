                               /*border-bottom: 6px solid #126d9b;
                               margin-bottom: -8px;*/
                           }

                               nav ul li:hover:after {
                                   -webkit-animation: circle 1.5s ease-in forwards;
                               }

                           nav ul li ul {
                               border-bottom: 5px solid #2ba0db;
                               display: none;
                               position: absolute;
                               width: 250px;
                           }

                               nav ul li ul li {
